{
  "manifest_version": 3,
  "name": "OpenFront Population Display",
  "version": "1.0",
  "description": "Displays population value from OpenFront.io games",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://openfront.io/*"],
      "css": ["styles.css"],
      "js": ["alert.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["mp3/*", "img/characters/*"],
      "matches": ["https://openfront.io/*"]
    }
  ]
}
